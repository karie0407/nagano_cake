<h4>配送先編集</h4>
<%= form_with model: @address, url: address_path(@address.id), method: :patch do |f| %>
<%= f.label "郵便番号" %>
<%= f.text_field :postal_code %>
<%= f.label "住所" %>
<%= f.text_field :address %>
<%= f.label "宛名" %>
<%= f.text_field :name %>
<%= f.submit "変更内容を保存", class: "btn btn-action col-md-2 offset-md-3 mt-1 mb-1" %>

<% end %>